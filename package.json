{
  "name": "es6-npm-boilerplate",
  "version": "0.1.2",
  "author": "Albert Gao",
  "main": "dist/index.js",
  "description": "A NPM package ES6 boilerplate ready to use .",
  "contributors": [
    {
      "name": "Albert Gao",
      "email": "albertgaohy@gmail.com"
    }
  ],
  "scripts": {
    "lint": "eslint src test",
    "clean": "rimraf dist/",
    "build": "rimraf dist/ && babel src/ --out-dir dist/",
    "test": "./node_modules/.bin/jest || true",
    "example": "node example.js",
    "prepublish": "npm run clean && npm run lint && npm run test && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Mr-Binary/es6-npm-boilerplate.git"
  },
  "keywords": [
    "module",
    "es6",
    "es5",
    "npm",
    "babel"
  ],
  "devDependencies": {
    "babel-cli": "6.16.0",
    "babel-core": "6.16.0",
    "babel-eslint": "7.0.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.0",
    "eslint": "3.6.1",
    "eslint-config-airbnb": "12.0.0",
    "eslint-config-prettier": "^2.6.0",
    "eslint-plugin-import": "1.16.0",
    "eslint-plugin-jsx-a11y": "2.2.2",
    "eslint-plugin-react": "6.3.0",
    "jest": "^21.2.1",
    "rimraf": "2.5.4",
    "run-sequence": "1.2.2"
  },
  "engine": "node >= 6.5.x",
  "homepage": "https://github.com/Mr-Binary/es6-npm-boilerplate#readme",
  "bugs": {
    "url": "https://github.com/Mr-Binary/es6-npm-boilerplate/issues"
  },
  "license": "MIT",
  "prettier": {
    "singleQuote": true
  }
}
